<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.RunImprove">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RunImprove">
        <activity
            android:name=".HistoricalActivity"
            android:exported="false"
            android:label="Historial"/>
        <activity
            android:name=".DiaryActivity"
            android:exported="false"
            android:label="Entreno Diario" />
        <activity
            android:name=".RoutinesActivity"
            android:exported="false"
            android:label="Rutinas" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:icon="@drawable/actionbar"
            android:label="RunImprove">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>